{
  "name": "headless-coding-agent",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "coding-agent": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && chmod +x dist/cli.js",
    "dev": "npx tsx src/cli.ts"
  },
  "devDependencies": {
    "@codesandbox/sdk": "^2.0.7",
    "@types/node": "^22.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.5.0",
    "@types/jsdom": "^21.1.7"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.4",
    "@ai-sdk/google": "^2.0.6",
    "@ai-sdk/mistral": "^2.0.5",
    "@ai-sdk/openai": "^2.0.15",
    "@ai-sdk/togetherai": "^1.0.7",
    "@ai-sdk/xai": "^2.0.7",
    "@mozilla/readability": "^0.6.0",
    "ai": "^5.0.15",
    "jsdom": "^26.1.0",
    "lru-cache": "^11.1.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  }
}
